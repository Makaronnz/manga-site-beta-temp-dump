{
  "name": "manga-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "fix:pages": "node scripts/fix-open-next.js",
    "export:cf": "npx @cloudflare/next-on-pages@latest",
    "preview": "npx @opennextjs/cloudflare build && npx @opennextjs/cloudflare preview",
    "deploy": "npx @opennextjs/cloudflare build && npx @opennextjs/cloudflare deploy",
    "deploy:cf": "npx @opennextjs/cloudflare build && npm run fix:pages && wrangler pages deploy .open-next --project-name=manga-site"
  },
  "dependencies": {
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^1.8.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.37.1"
  }
}
